generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model animal_categories {
  id            Int             @id @default(autoincrement())
  category      String?         @unique(map: "category_unique") @db.VarChar(255)
  animal_photos animal_photos[]
}

model animal_photos {
  id                Int               @id @default(autoincrement())
  category_id       Int               @default(autoincrement())
  photo_url         String?
  animal_categories animal_categories @relation(fields: [category_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
